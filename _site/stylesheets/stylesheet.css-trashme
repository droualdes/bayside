font-style: italic;
}
h5 {
font-size: .95em;
font-weight: 700;
line-height: 100%;
margin: 10px 0 15px 0;
}
h6 {
font-size: .95em;
font-weight: 700;
line-height: 100%;
margin: 10px 0 15px 0;
}
a, a:visited {
color: #708d6e;
text-decoration: none;
}
a:hover, a:active {
color: #708d6e;;
text-decoration: none;
}
.clear {
clear: both;
}
ul {
display: inline-block;
font-size: .95em;
list-style-type: square;
padding: 0 0 0px 15px;
margin: 0;
}
